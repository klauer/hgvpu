  field(FLNK, "$(U):USAAmountToMove")
  field(CALC,"(K<=L&I<J)?0:ABS(A)>E?B+F*A:B+A")
  field(INPA,"$(U):USWMotAmountToMove.VAL PP NMS")
  field(INPB,"$(U):USWMotor.RBV NPP NMS")
  field(INPE,"0.02")
  field(INPF,"0.9")
  field(INPI,"$(U):GapSymmetry.VAL NPP NMS")
