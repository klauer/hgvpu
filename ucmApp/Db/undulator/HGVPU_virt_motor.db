
record(motor, "$(U):$(AXIS)Motor") {
  field(SCAN, "Passive")
  field(DTYP, "asynMotor")
  field(DISS, "NO_ALARM")
  field(DIR, "$(DIR)")
  field(VELO, "$(VELO)")
  field(VBAS, "$(VBAS)")
  field(VMAX, "$(VMAX)")
  field(URIP, "Yes")
  field(PREC, "$(PREC)")
  field(EGU, "$(EGU)")
  field(RRES, "$(RRES)")
  field(OUT, "@asyn($(PORT),$(ADDR))")
  field(RTRY, "0")
  field(TWV, "$(TWV)")
  field(ASG, "MC_MAINT")
  field(FOFF, "Frozen")
  field(PRIO, "HIGH")
  field(OMSL, "closed_loop")
  field(RDBL, "$(U):$(RDBL) CP MS")
  field(DESC, "$(DESC)")
  field(MRES, "$(MRES)")
  field(DHLM, "$(DHLM)")
  field(DLLM, "$(DLLM)")
  field(RDBD, "$(MRES)")
}

record(ao, "$(U):$(AXIS)Push") {
  field(DOL, "$(U):$(AXIS)Motor.VAL CP NMS")
  field(OMSL, "closed_loop")
  field(DESC, "Push Value to Desired Existing PV")
  field(OUT, "$(U):$(DEST) PP NMS")
  field(PREC, "$(PREC)")
  field(EGU, "$(EGU)")
}

record(bo, "$(U):$(AXIS)Motor:Defer") {
  field(DTYP, "asynInt32")
  field(OUT, "@asynMask($(PORT),$(ADDR),1)MOTOR_DEFER_MOVES")
  field(ZNAM, "Go")
  field(ONAM, "Defer")
  field(PINI, "YES")
  field(VAL, "0")
}

record(longin, "$(U):$(AXIS):StatusWD") {
  field(DESC, "WatchDog Status")
  field(SCAN, "I/O Intr")
  field(DTYP, "asynInt32")
  field(INP, "@asyn($(PORT),$(ADDR))WD_STATUS")
}

record(longin, "$(U):$(AXIS):StatusSync") {
  field(DESC, "Trajectory Monitor Bit")
  field(SCAN, "I/O Intr")
  field(DTYP, "asynInt32")
  field(INP, "@asyn($(PORT),$(ADDR))SYNC_STATUS")
}


record(seq, "$(U):$(AXIS)Motor:Trig") {
  field(DESC, "Trigger Deffer")
  field(DOL1, "1")
  field(LNK1, "$(U):$(AXIS)Motor:Defer PP")
  field(DLY2, "0.2")
  field(DOL2, "0")
  field(LNK2, "$(U):$(AXIS)Motor:Defer PP")
}

